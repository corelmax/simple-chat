{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "member-access": false,
        "new-parens": false,
        "no-console": false,
        "max-classes-per-file": false,
        "align": false,
        "no-empty": false,
        "no-var-requires": false,
        "max-line-length": false,
        "no-namespace": false
    },
    "rulesDirectory": []
}